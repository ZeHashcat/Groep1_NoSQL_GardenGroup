<Page x:Class="GardenGroupUI.AppMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GardenGroupUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage">

    <Grid>
        <TabControl Margin="10,20,10,10">
            <TabItem Header="Dashboard">
                <Frame Source="../Dashboard/DashboardPage.xaml" Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Incident Management">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Manager Management">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
