<Page x:Class="GardenGroupUI.Dashboard.DashboardEmployeePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GardenGroupUI.Dashboard"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DashboardEmployeePage">

    <ScrollViewer VerticalScrollBarVisibility="Auto" VerticalAlignment="Stretch">
        <StackPanel>
            <Frame Source="DashboardToolbarUserControl.xaml"/>
            <WrapPanel Orientation="Horizontal" x:Name="DashboardWrapPanel"/>
            <Grid Height="300" HorizontalAlignment="Center" Width="700">
                <Image HorizontalAlignment="Left" Height="250" Margin="39,30,0,0" VerticalAlignment="Top" Width="250" Source="/Dashboard/graph 1.JPG"/>
                <Image HorizontalAlignment="Left" Height="250" Margin="320,30,0,0" VerticalAlignment="Top" Width="250" Source="/Dashboard/graph 2.JPG"/>
            </Grid>
        </StackPanel>
    </ScrollViewer>
</Page>
