<Page x:Class="GardenGroupUI.Dashboard.DashboardEmployeePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GardenGroupUI.Dashboard"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DashboardEmployeePage">

    <ScrollViewer VerticalScrollBarVisibility="Auto" VerticalAlignment="Stretch">
        <StackPanel>
            <Frame Source="DashboardToolbarUserControl.xaml"/>
            <WrapPanel Orientation="Horizontal" x:Name="DashboardWrapPanel"/>
        </StackPanel>
    </ScrollViewer>
</Page>
